{
  "name": "@raydeck/serverless-lambda-builder",
  "version": "2.2.2",
  "description": "Build lambda definitiosn for serverless",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "tsc && chmod +x lib/bin.js",
    "docs": "typedoc --theme markdown --readme none --mode file --excludeNotExported --includeVersion --out docstemp && commanderdoc markdown lib/bin.js -o docstemp/__cli.md --cli-name \"npx `npx jq -r .name package.json`\" && rm README.md ; mv docstemp/README.md docstemp/_library.md && concat-md docstemp > README.md && rm -rf docstemp",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhdeck/serverless-lambda-builder.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhdeck/serverless-lambda-builder/issues"
  },
  "homepage": "https://github.com/rhdeck/serverless-lambda-builder#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@raydeck/livelink": "^1.5.2",
    "@types/aws-lambda": "^8.10.59",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.0.27",
    "commanderdoc": "^0.1.4",
    "concat-md": "^0.3.5",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.4.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "src",
    "lib",
    "templates"
  ],
  "dependencies": {
    "@types/prettier": "^2.0.2",
    "commander": "^6.0.0",
    "install-peers-cli": "^2.2.0",
    "mustache": "^4.0.1",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "yaml": "^1.10.0"
  }
}
